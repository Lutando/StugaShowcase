{
  "name": "fabe.github.io",
  "version": "1.0.0",
  "description": "My new site!",
  "scripts": {
    "watch:css": "postcss _assets/src/style.css -o _assets/css/style.css -w",
    "watch:scripts": "webpack -d -w --module-bind 'js=babel-loader' --output-filename _assets/js/bundle.js _assets/js/app.js",
    "watch:jekyll": "bundle exec jekyll server -L",
    "build:css": "postcss _assets/src/style.css -o _assets/css/style.css",
    "build:scripts": "webpack -p --module-bind 'js=babel-loader' --output-filename _assets/js/bundle.js _assets/js/app.js",
    "build:jekyll": "bundle exec jekyll build",
    "build": "npm run build:css && npm run build:scripts && npm run build:jekyll",
    "dev": "npm run watch:css & npm run watch:scripts & npm run watch:jekyll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabe/fabe.github.io.git"
  },
  "authors": [
    {
      "name": "Fabian Schultz",
      "email": "desk@fabianschultz.com"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "postcss-cli": "^4.1.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-csso": "^2.0.0",
    "postcss-import": "^10.0.0",
    "webpack": "^3.5.4"
  },
  "dependencies": {
    "stickybits": "^1.5.3"
  }
}
